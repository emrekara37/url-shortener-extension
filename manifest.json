{
    "manifest_version": 2,
    "name": "Url Shortener",
    "description": "Shorten active url and generate qrcode",
    "version": "1.0",
    "browser_action": {
        "default_title": "Url Shortener",
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "64": "64.png"
    }
}